{
  "manifest_version": 2,

  "name": "lite-player",
  "description": "This extension give to easy listen music from vk",
  "version": "0.0.1",
  "author": "Matsitsky S.S.",
  "minimum_chrome_version": "48",
  "browser_action": {
    "default_title": "lite player",
    "default_icon": "images.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "http://www.youtube.com/",
    "http://www.youtube.com/player_api/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
