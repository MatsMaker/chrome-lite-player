{
  "manifest_version": 2,

  "name": "lite-player",
  "description": "This extension give to easy listen music from vk",
  "version": "0.0.1",
  "author": "Matsitsky S.S.",
  "minimum_chrome_version": "48",
  "browser_action": {
    "default_title": "lite player",
    "default_icon": "img/mp3-player-icon-x48.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "https://w.soundcloud.com/*",
    "https://soundcloud.com/*"
  ],
  "icons": {
    "16": "img/mp3-player-icon-x16.png",
    "48": "img/mp3-player-icon-x48.png",
    "128": "img/mp3-player-icon-x128.png"
  }
}
